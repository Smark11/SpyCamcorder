        <trans-unit id="Resx/Instruction1" translate="yes" xml:space="preserve">
          <source>Tap the settings screen to view the settings.</source>
          <target state="needs-review-translation" state-qualifier="mt-suggestion">点击设置屏幕，查看设置。</target>
        </trans-unit>
        <trans-unit id="Resx/Instruction2" translate="yes" xml:space="preserve">
